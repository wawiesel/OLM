{
    "model": {
        "name": "w17x17_pin2",
        "work_dir": "_build",
        "scale_env_var": "SCALE",
        "description": "A 2D t-depl quarter assembly model for a Pressurized Water Reactor of the Westinghouse 17x17 type.",
        "sources":
        {
            "1":"Characteristics of Spent Fuel, High-Level Waste, and other Radioactive Wastes which May Require Long-Term Isolation, Appendix 2A. Physical Descriptions of LWR Fuel Assemblies, DOE/RW-0184, Volume 3 of 6, U.S. DOE Office of Civilian Radioactive Waste Management, 1987.",
            "2": "SCALE: A Comprehensive Modeling and Simulation Suite for Nuclear Safety Analysis and Design, ORNL/TM-2005/39, Version 6.1, Oak Ridge National Laboratory, Oak Ridge, Tennessee, June 2011.",
            "3": "H. Smith, J. Peterson, and J. Hu, Fuel Assembly Modeling for the Modeling and Simulation Toolset, ORNL/LTR-2012-555 Rev. 1, Oak Ridge National Laboratory, 2013."
        }
    },
    "generate": {
        "_type": "scale.olm.generate:expander",
        "template": "model.inp",
        "comp": {
            "_type": "scale.olm.generate:fuelcomp_uox_nuregcr5625",
            "nuclide_prefix": "wt_"
        },
        "params": {
            "_type": "scale.olm.common:pass_through",
            "addnux": 2,
            "xslib": "v7-56",
            "pitch": 1.26,
            "fuelr": 0.4095,
            "cladr": 0.4750
        },
        "states": {
            "_type": "scale.olm.generate:all_permutations",
            "coolant_density": [
                0.70,
                0.72,
                0.74
            ],
            "enrichment": [
                0.5,
                3,
                5,
                7
            ],
            "specific_power": [
                40
            ],
            "boron_ppm": [ 600 ]
        },
        "time": {
            "_type": "scale.olm.generate:triton_constpower_burndata",
            "gwd_burnups": [
                0.0,
                1.0,
                10.0,
                25.0,
                50.0,
                70.0,
                90.0
            ]
        }
    },
    "run": {
        "_type": "scale.olm.run:makefile",
        "nprocs": 3,
        "dry_run": false
    },
    "build": {
        "_type": "scale.olm.build:arpdata_txt",
        "fuel_type": "UOX",
        "suffix": ".system.f33",
        "dim_map":
        {
            "enrichment": "enrichment",
            "mod_dens": "coolant_density"
        },
        "keep_every": 2
    },
    "check": {
        "_type": "scale.olm.check:sequencer",
        "sequence":
        [
        {
            "template": "check-origami.inp",
            "_type": "scale.olm.check:LowOrderConsistency",
            "target_q1": 0.75,
            "target_q2": 0.96,
            "eps0": 1e-12,
            "epsa": 1e-6,
            "epsr": 1e-3
        }
        ]
    },
    "report": {
        "_type": "scale.olm.report:stub1",
        "template": "report.rst"
    }
}
