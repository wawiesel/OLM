{
    "model": {
        "name": "mox_w17x17_pin2",
        "work_dir": "_build",
        "scale_env_var": "SCALE",
        "description": "A really simple MOX pin.",
        "sources": { "1": "personal communication" }
    },
    "generate": {
        "_type": "scale.olm.generate:expander",
        "template": "model.inp",
        "comp": {
            "inner":
            {
                "_type": "scale.olm.generate:comp_mox_ornltm2003_2",
                "density": 10.4,
                "uo2": {
                    "iso": {
                        "u234": 0.001,
                        "u235": 0.24,
                        "u236": 0.001,
                        "u238": 99.75
                    }
                },
                "am241": 1.0
            }
         },
        "params": {
            "_type": "scale.olm.common:pass_through",
            "addnux": 2,
            "xslib": "v7-56",
            "pitch": 1.26,
            "fuelr": 0.4095,
            "cladr": 0.4750
        },
        "states": {
            "_type": "scale.olm.generate:all_permutations",
            "coolant_density": [
                0.65,
                0.75
            ],
            "pu_frac": [
                4.0,
                10.0
            ],
            "pu239_frac": [
                50.0,
                60.0,
                70.0
            ],
            "specific_power": [
                40
            ],
            "boron_ppm": [ 600 ]
        },
        "time": {
            "_type": "scale.olm.generate:triton_constpower_burndata",
            "gwd_burnups": [
                0.0,
                1.0,
                25.0,
                50.0,
                90.0
            ]
        }
    },
    "run": {
        "_type": "scale.olm.run:makefile",
        "nprocs": 3,
        "dry_run": false
    },
    "build": {
        "_type": "scale.olm.build:arpdata_txt",
        "fuel_type": "MOX",
        "dim_map":
        {
            "pu_frac": "pu_frac",
            "pu239_frac": "pu239_frac",
            "mod_dens": "coolant_density"
        },
        "keep_every": 1
    },
    "check": {
        "_type": "scale.olm.check:sequencer",
        "nprocs": 3,
        "sequence":
        [{
            "template": "check-origami.inp",
            "_type": "scale.olm.check:LowOrderConsistency",
            "target_q1": 0.75,
            "target_q2": 0.96,
            "eps0": 1e-12,
            "epsa": 1e-6,
            "epsr": 1e-3
        }]
    },
    "report": {
        "_type": "scale.olm.report:stub1",
        "template": "report.rst"
    }
}
