  cuboid   40 4p0.643
  media 1 1 10
  media 4 1 20 -10
  media 2 1 30 -20
  media 3 1 40 -30
  boundary 40
unit 51
com='1/4 guide tube - NE'
  cylinder 10 1.143  chord +x=0 chord +y=0
  cylinder 20 1.2446 chord +x=0 chord +y=0
  cuboid   30 1.286  0 1.286 0
  media 3 1 10
  media 5 1 20 -10
  media 3 1 30 -20 
