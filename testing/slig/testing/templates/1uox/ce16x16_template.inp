'  figure{ce16x16.pdf: PWR CE 16x16 fuel assembly.}
' ----------------------------------------------------------------
read comp
'
' fuel - 94-95% theoretical density (10.96g/cc) (set to max)
  uo2   1 den=10.412 1 1000 92234 u234wt% 
                            92235 u235wt% 
                            92236 u236wt% 
                            92238 u238wt% end 
'
' clad
  zirc4 2 1 620 end 
'
' coolant - water with 650 ppm soluble boron 
  h2o   3 den=ddd 1.000000 600 end
  boron 3 den=ddd 0.000650 600 end
'
' gap - 300-450psig (set at minimum)
  he-4  4 den=0.001605 1 620 end
'
' guide tube
  zirc4 5 1 600 end 
end comp
'
' ----------------------------------------------------------------
read celldata
  latticecell squarepitch fueld=0.8255  1
                          gapd= 0.84328 4
                          cladd=0.97028 2
                          pitch=1.286   3 end
  centrmdata alump=0 pmc_dilute=1e30 pmc_omit=0 end centrmdata
end celldata
'
' ----------------------------------------------------------------
read depletion 
  1 
end depletion
'
' ----------------------------------------------------------------
read burndata
  power=specpow burn=daystoburn down=0 end
end burndata
'
' ----------------------------------------------------------------
read model
  PWR CE 16x16  
'
read parm
  cmfd=1
  drawit=yes
  echo=yes
  epsinner=-1e-4 
  fillmix=3 
  run=yes
  xycmfd=4
  sn=8
  cell_tol=1e-8
end parm
'
read materials
  mix=1 pn=1 com='fuel'       end
  mix=2 pn=1 com='clad'       end
  mix=3 pn=2 com='coolant'    end
  mix=4 pn=0 com='gap'        end
  mix=5 pn=1 com='guide tube' end
end materials
'
read geom
unit 1
com='fuel rod'
