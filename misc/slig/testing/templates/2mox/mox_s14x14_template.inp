read alias
  $fuel 1 11 21 31 end
  $clad 2 12 22 32 end
  $mod  3 13 23 33 end
  $gap  4 14 24 34 end
end alias
'
' ----------------------------------------------------------------
read comp
'
' fuel inner
  uo2  1 den=10.4 IcontentU  859 92234 0.001 92235 0.24 92238 99.759 end
  puo2 1 den=10.4 IcontentPu 859 94238 pu238wt%
                                 94239 pu239wt%
                                 94240 pu240wt%
                                 94241 pu241wt%
                                 94242 pu242wt% end
  am-241 1 den=densityAm IcontentPu 859 end
'
' fuel inside edge
  uo2  11 den=10.4 IEcontentU  859 92234 0.001 92235 0.24 92238 99.759 end
  puo2 11 den=10.4 IEcontentPu 859 94238 pu238wt%
                                   94239 pu239wt%
                                   94240 pu240wt%
                                   94241 pu241wt%
                                   94242 pu242wt% end
  am-241 11 den=densityAm IEcontentPu 859 end
'
' fuel edge
  uo2  21 den=10.4 EcontentU  859 92234 0.001 92235 0.24 92238 99.759 end
  puo2 21 den=10.4 EcontentPu 859 94238 pu238wt%
                                  94239 pu239wt%
                                  94240 pu240wt%
                                  94241 pu241wt%
                                  94242 pu242wt% end
  am-241 21 den=densityAm EcontentPu 859 end
'
' fuel corner
  uo2  31 den=10.4 CcontentU  859 92234 0.001 92235 0.24 92238 99.759 end
  puo2 31 den=10.4 CcontentPu 859 94238 pu238wt%
                                  94239 pu239wt%
                                  94240 pu240wt%
                                  94241 pu241wt%
                                  94242 pu242wt% end
  am-241 31 den=densityAm CcontentPu 859 end
'
' clad
  zirc4 $clad 1 605 end
'
' coolant - water with 450 ppm soluble boron
  h2o   $mod den=ddd 1.000000 572 end
  boron $mod den=ddd 0.000450 572 end 
'
' gap
  n     $gap den=0.00125 1 605 end
'
' guide tube
  zirc4 5 1 572 end
end comp
'
' ----------------------------------------------------------------
read celldata
  latticecell squarepitch fueld=0.925 $fuel
                          gapd= 0.930 $gap
                          cladd=1.071 $clad
                          pitch=1.430 $mod  end
Â  centrmdata alump=0 pmc_dilute=1e30 pmc_omit=0 end centrmdata
end celldata
'
' ----------------------------------------------------------------
read depletion 
  $fuel
end depletion
'
' ----------------------------------------------------------------
