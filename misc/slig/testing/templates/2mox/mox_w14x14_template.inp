read comp
'
' fuel inner
  uo2  1 den=10.4 IcontentU  923 92234 0.001 92235 0.24 92238 99.759 end
  puo2 1 den=10.4 IcontentPu 923 94238 pu238wt%
                                 94239 pu239wt%
                                 94240 pu240wt%
                                 94241 pu241wt%
                                 94242 pu242wt% end
  am-241 1 den=densityAm IcontentPu 923 end
'
' fuel inside edge
