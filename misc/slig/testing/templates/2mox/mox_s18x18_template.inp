  cylinder 20   0.475
  cuboid   30 4p0.635
  media 21 1 10
  media  2 1 20 -10
  media  3 1 30 -20 
  boundary 30
unit 4
com='fuel rod - corner zone'
  cylinder 10   0.411
  cylinder 20   0.475
  cuboid   30 4p0.635
  media 31 1 10
  media  2 1 20 -10
  media  3 1 30 -20 
  boundary 30
unit 5
com='guide tube'
  cylinder 10    0.555
  cylinder 20    0.616
  cuboid   40  4p0.635
  media 3 1 10
  media 5 1 20 -10
  media 3 1 40 -20
  boundary  40
global unit 10
com='1/4 assembly - NE'
  cuboid 10 11.47318 0 11.47318 0
  array 1 10 place 1 1 0.635 0.635
  media 3 1 10
  boundary 10 54 54
end geom
'
read array 
  ara=1 nux=9 nuy=9 
  fill
  1   1   1   1   1   1   1   2   3
  1   1   1   1   5   1   1   2   3
  1   1   1   1   1   1   5   2   3
  1   1   1   5   1   1   1   2   3
  1   5   1   1   1   1   1   2   3
  1   1   1   1   1   5   1   2   3
  1   1   5   1   1   1   1   2   3
  2   2   2   2   2   2   2   2   4
  3   3   3   3   3   3   3   4   4  end fill
end array
'
read bnds
  all=mirror
end bnds
'
