  uo2  31 den={{comp.corner.density}} {{comp.corner.uo2.dens_frac}} 923 92234 0.001 92235 0.24 92238 99.759 end
  puo2 31 den={{comp.corner.density}} {{comp.corner.puo2.dens_frac}} 923
                                 94238 {{comp.corner.puo2.iso.pu238}}
                                 94239 {{comp.corner.puo2.iso.pu239}}
                                 94240 {{comp.corner.puo2.iso.pu240}}
                                 94241 {{comp.corner.puo2.iso.pu241}}
                                 94242 {{comp.corner.puo2.iso.pu242}} end
