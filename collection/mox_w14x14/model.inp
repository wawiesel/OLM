read alias
  $fuel 1 11 21 31 end
  $clad 2 12 22 32 end
  $mod  3 13 23 33 end
  $gap  4 14 24 34 end
end alias
'
' ----------------------------------------------------------------
read comp

' fuel inner
  uo2  1 den={{comp.inner.density}} {{comp.inner.uo2.dens_frac}} 923 92234 0.001 92235 0.24 92238 99.759 end
  puo2 1 den={{comp.inner.density}} {{comp.inner.puo2.dens_frac}} 923
                                 94238 {{comp.inner.puo2.iso.pu238}}
                                 94239 {{comp.inner.puo2.iso.pu239}}
                                 94240 {{comp.inner.puo2.iso.pu240}}
                                 94241 {{comp.inner.puo2.iso.pu241}}
                                 94242 {{comp.inner.puo2.iso.pu242}} end
  am   1 den={{comp.inner.density}}
        {{comp.inner.amo2.dens_frac*comp.inner.info.amo2_hm_frac}} 923
        95241 {{comp.inner.amo2.iso.am241}} end
  o    1 den={{comp.inner.density}}
        {{comp.inner.amo2.dens_frac*(1.0-comp.inner.info.amo2_hm_frac)}} 923 end

'
' fuel inside edge
  uo2  11 den={{comp.iedge.density}} {{comp.iedge.uo2.dens_frac}} 923 92234 0.001 92235 0.24 92238 99.759 end
