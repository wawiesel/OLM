        {{comp.corner.amo2.dens_frac*comp.corner.info.amo2_hm_frac}} 923
        95241 {{comp.corner.amo2.iso.am241}} end
  o    31 den={{comp.corner.density}}
        {{comp.corner.amo2.dens_frac*(1.0-comp.corner.info.amo2_hm_frac)}} 923 end
'
' clad
  zirc4 $clad 1 595 end
'
' coolant - water with {{state.ppm_boron}} ppm soluble boron
  h2o   $mod den={{state.coolant_density}} 1.000000 573.4 end
  boron $mod den={{state.coolant_density}} {{state.ppm_boron/1.0E6}} 573.4 end
'
' gap - 300-450psig (set at minimum)
  he-4  $gap den=0.001672 1 595 end
'
' guide tube
  zirc4 5 1 573.4 end
'
end comp
'
' ----------------------------------------------------------------
read celldata
  latticecell squarepitch fueld=0.929386 $fuel
                          gapd= 0.948436 $gap
                          cladd=1.07188  $clad
                          pitch=1.43002  $mod  end
  centrmdata alump=0 pmc_dilute=1e30 pmc_omit=0 end centrmdata
end celldata
'
' ----------------------------------------------------------------
read depletion
