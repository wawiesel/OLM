        {{comp.iedge.amo2.dens_frac*comp.iedge.info.amo2_hm_frac}} 923
        95241 {{comp.iedge.amo2.iso.am241}} end
  o    11 den={{comp.iedge.density}}
        {{comp.iedge.amo2.dens_frac*(1.0-comp.iedge.info.amo2_hm_frac)}} 923 end

'
' fuel edge
  uo2  21 den={{comp.edge.density}} {{comp.edge.uo2.dens_frac}} 923 92234 0.001 92235 0.24 92238 99.759 end
